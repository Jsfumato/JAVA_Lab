package num6;

import java.io.IOException;
import java.io.InputStreamReader;

public class Main {

	public static void main(String[] args) {
		
		while(true){
			InputStreamReader irs = new InputStreamReader(System.in);
			try {
				int input = irs.read();
				System.out.println(input);
			} catch (IOException e) {
				e.printStackTrace();
			}
//			if(input == "E"){
//				break;
//			}
			
		}
	}
}
