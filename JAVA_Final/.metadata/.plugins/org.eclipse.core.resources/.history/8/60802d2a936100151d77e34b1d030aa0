package num1;

public class CarFactory {

	private static CarFactory cf = new CarFactory();
	private CarFactory(){};
	
	public static CarFactory getInstance(){
		if(cf == null){
			CarFactory cf = new CarFactory();
			return cf;
		}
		return cf;
	}
	
	public Car createCar(String carname){
		Car car = new Car(carname);
		return car;
	}
	
}
