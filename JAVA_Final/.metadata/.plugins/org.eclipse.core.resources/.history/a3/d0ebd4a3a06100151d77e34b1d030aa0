import java.io.InputStreamReader;
import java.nio.CharBuffer;

public class Main {

	public static void main(String[] args) {
		
		while(true){
			InputStreamReader irs = new InputStreamReader(System.in);
			try {
				int input = irs.read();
				System.out.println((char)input);
				
				if((char)input == "E"){
					break;
				}
				
				if((char)input == "S"){
					
				}
				if((char)input == "Q"){
						
				}
				if((char)input == "M"){
							
				}
				
			} catch (IOException e) {
				e.printStackTrace();
			}

			
		}
	}
}
