package num4;

public abstract class Car {
	
	private String name;
	
	public Car(String name){
		this.name = name;
	}
	
	private void start(){
		System.out.println(name +" start!");
	}
	private void drive(){
		System.out.println(name +" drive!");
	}
	private void stop(){
		System.out.println(name +" stop!");
	}
	private void turnoff(){
		System.out.println(name +" turnoff!");
	}
	
	public void run(){
		this.start();
		this.drive();
		this.stop();
		this.turnoff();
		System.out.println();
	}
	
	public void washCar(){
		System.out.println("WashCar!");
	}
}

